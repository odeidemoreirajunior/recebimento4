<core:FragmentDefinition xmlns:core="sap.ui.core" 
	xmlns:t="sap.ui.table"
	 xmlns:f="sap.ui.layout.form"
     xmlns="sap.m"
     
     xmlns:html="http://www.w3.org/1999/xhtml">

    <Dialog title = "{i18n>titleItem}" 
            beforeOpen="onBeforeOpen" class="dialog"  >
    
    <Table id="idTableItem"         
        items="{/lista}">
        <columns>
            <Column>
                <Text text="Lote" />
            </Column>          
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <Text
                        text="{/Lote}" />
                </cells>
            </ColumnListItem>
        </items>
    </Table>
     <buttons>
            <Button
                
                enabled="true"
               
                iconDensityAware="true"
                iconFirst="true"
                text="Fechar"
                press="onCancel"
                textDirection="Inherit"
                type="Back"
                 />

       </buttons>
   </Dialog>
  


</core:FragmentDefinition>